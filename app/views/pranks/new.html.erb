<div class="container">
  <h1>Add a prank!</h1>
  <%= simple_form_for [@prank] do |f| %>
    <%= f.input :name %>
    <%= f.input :content %>
    <%= f.input :price %>
    <%= f.input :city %>
    <%= f.input :photo, as: :file, inline_label: 'photo', label: false %>
    <%= f.submit 'Add', class: 'btn btn-flat' %>
  <% end %>
</div>
